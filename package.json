{
  "name": "svero",
  "version": "0.1.7",
  "description": "Simple Router for Svelte 3",
  "main": "public/svero.min.js",
  "files": [
    "public/svero.min.js",
    "public/svero.min.js.map",
    "README.md",
    "LICENSE"
