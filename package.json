  "svelte": "src/main.js",
  "files": [
    "src/*",
    "README.md",
    "LICENSE"
  ],
  "peerDependencies": {
    "svelte": "3.x"
  },
  "devDependencies": {
    "chromedriver": "^74.0.0",
